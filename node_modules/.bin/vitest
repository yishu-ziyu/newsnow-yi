#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*|*MINGW*|*MSYS*)
        if command -v cygpath > /dev/null 2>&1; then
            basedir=`cygpath -w "$basedir"`
        fi
    ;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/mahaoxuan/Desktop/vibe coding/readnew/node_modules/.pnpm/vitest@3.1.3_@types+node@22.15.17_jiti@2.4.2_terser@5.39.0_tsx@4.19.4_yaml@2.7.1/node_modules/vitest/node_modules:/Users/mahaoxuan/Desktop/vibe coding/readnew/node_modules/.pnpm/vitest@3.1.3_@types+node@22.15.17_jiti@2.4.2_terser@5.39.0_tsx@4.19.4_yaml@2.7.1/node_modules:/Users/mahaoxuan/Desktop/vibe coding/readnew/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/Users/mahaoxuan/Desktop/vibe coding/readnew/node_modules/.pnpm/vitest@3.1.3_@types+node@22.15.17_jiti@2.4.2_terser@5.39.0_tsx@4.19.4_yaml@2.7.1/node_modules/vitest/node_modules:/Users/mahaoxuan/Desktop/vibe coding/readnew/node_modules/.pnpm/vitest@3.1.3_@types+node@22.15.17_jiti@2.4.2_terser@5.39.0_tsx@4.19.4_yaml@2.7.1/node_modules:/Users/mahaoxuan/Desktop/vibe coding/readnew/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../vitest/vitest.mjs" "$@"
else
  exec node  "$basedir/../vitest/vitest.mjs" "$@"
fi
