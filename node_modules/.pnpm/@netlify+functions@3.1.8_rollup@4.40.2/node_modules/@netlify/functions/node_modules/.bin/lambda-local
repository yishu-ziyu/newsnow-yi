#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*|*MINGW*|*MSYS*)
        if command -v cygpath > /dev/null 2>&1; then
            basedir=`cygpath -w "$basedir"`
        fi
    ;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/mahaoxuan/Desktop/vibe coding/readnew/node_modules/.pnpm/lambda-local@2.2.0/node_modules/lambda-local/build/node_modules:/Users/mahaoxuan/Desktop/vibe coding/readnew/node_modules/.pnpm/lambda-local@2.2.0/node_modules/lambda-local/node_modules:/Users/mahaoxuan/Desktop/vibe coding/readnew/node_modules/.pnpm/lambda-local@2.2.0/node_modules:/Users/mahaoxuan/Desktop/vibe coding/readnew/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/Users/mahaoxuan/Desktop/vibe coding/readnew/node_modules/.pnpm/lambda-local@2.2.0/node_modules/lambda-local/build/node_modules:/Users/mahaoxuan/Desktop/vibe coding/readnew/node_modules/.pnpm/lambda-local@2.2.0/node_modules/lambda-local/node_modules:/Users/mahaoxuan/Desktop/vibe coding/readnew/node_modules/.pnpm/lambda-local@2.2.0/node_modules:/Users/mahaoxuan/Desktop/vibe coding/readnew/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../../../../../lambda-local@2.2.0/node_modules/lambda-local/build/cli.js" "$@"
else
  exec node  "$basedir/../../../../../../lambda-local@2.2.0/node_modules/lambda-local/build/cli.js" "$@"
fi
